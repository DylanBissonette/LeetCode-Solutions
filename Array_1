/*
LeetCode challenge level: Easy
Completed on January 3rd 2026
*/

class Solution {
    public int[] getConcatenation(int[] nums) {
        int length = nums.length;
        int[] ans = new int[2 * length];
        // copy over nums into first half of ans, then second half
        for(int i = 0; i < length; i++) {
            ans[i] = nums[i];
            ans[i + length] = nums[i];
        }
        return ans;
    }
}
